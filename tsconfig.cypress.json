{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "cypress/**/*",
    "cypress.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}